pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
generation=0
dead=1
alive=2
tiles={alive, dead}

function _init()
end

function _update()
  generation+=1
end

function _draw()
  cls()
  draw_map()
  print("generation:"..tostr(generation))
end

function draw_map()
  for i=0,15 do
    for j=0,15 do
      local rand_tile_index = (flr(rnd()*10)%2)+1 
      spr(tiles[rand_tile_index], i*8, j*8)
    end
  end
end
